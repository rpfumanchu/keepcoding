
from sumas import sumaTodos,sumaTodosAlcuadrado,sumatorio

print(sumaTodos(15), sumatorio(100)) #da 5050

def cuadrado(x):
    return x**2

print(sumaTodosAlcuadrado(3),sumatorio(3)) #da 14

print(sumaTodosAlcuadrado(3),sumatorio(3, cuadrado), sumatorio(3, lambda x: x*x)) #da 14

print(sumatorio(15, lambda x: 2* x +1))